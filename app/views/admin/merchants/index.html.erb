<h1 style="text-align:left"> Admin Merchants </h1>

<%= render "/shared/admin_links" %><br>

<% @merchants.distinct.each do |merchant| %>
<ul class="list-group w-25 p-3">
  <li class="list-group-item list-group-item-secondary "><%=link_to "#{merchant.name}", "/admin/merchants/#{merchant.id}"%></li>
</ul>
<% end %>
